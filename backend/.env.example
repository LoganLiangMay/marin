# Application Configuration
APP_NAME="Audio Call Data Ingestion Pipeline"
APP_VERSION="1.0.0"
DEBUG=False

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# S3 Buckets (from Story 1.3)
S3_BUCKET_AUDIO=audio-pipeline-dev-audio
S3_BUCKET_TRANSCRIPTS=audio-pipeline-dev-transcripts

# MongoDB (from Story 1.4)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority
MONGODB_DATABASE=audio_pipeline

# Redis (from Story 1.5)
REDIS_ENDPOINT=your-redis-cluster.cache.amazonaws.com:6379
REDIS_PASSWORD=your-redis-password
REDIS_SSL=True

# SQS Queue (from Story 2.3)
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789/audio-processing-queue

# External APIs
OPENAI_API_KEY=sk-your-openai-api-key

# API Configuration
API_V1_PREFIX=/api/v1
CORS_ORIGINS=["http://localhost:3000","http://localhost:8000"]

# Server Configuration
HOST=0.0.0.0
PORT=8000
WORKERS=4
